{
  "name": "@vansergen/tor",
  "description": "Tor docker image",
  "version": "1.2.4",
  "private": true,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vansergen/tor.git"
  },
  "keywords": [
    "tor",
